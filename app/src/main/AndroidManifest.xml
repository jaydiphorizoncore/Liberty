<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Liberty"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".ui.dashboard.home.quiz.quizdetailsactivity.DailyQuizActivity"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.home.toppers.toppersdetailsactivity.ToppersActivity"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.home.testimonials.testimonialdetailsactivity.TestimonialActivity"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.home.category.categorydetailsactivity.CategoryActivity"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.home.courses.coursesdetailsactivity.CoursesActivity"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.home.packages.packagedetailsactivity.PackageActivity"
            android:exported="false" />
        <activity
            android:name=".ui.authentication.passwordactivity.PasswordActivity"
            android:exported="false" />
        <activity
            android:name=".ui.authentication.loginactivity.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.dashboard.MainActivity"
            android:exported="false" />
    </application>

</manifest>